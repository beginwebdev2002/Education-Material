{
	"servers": {
		"Github-MCP": {
			"url": "https://api.githubcopilot.com/mcp/",
			"type": "http"
		},
		"angular-cli": {
			"command": "npx",
			"args": [
				"-y",
				"@angular/cli",
				"mcp"
			]
		},
		"MongoDB": {
			"command": "npx",
			"args": [
				"-y",
				"mongodb-mcp-server@latest",
				"--readOnly"
			],
			"env": {
				"MDB_MCP_CONNECTION_STRING": "mongodb://localhost:27017/education-material"
			}
		},
		"chrome-devtools": {
			"command": "npx",
			"args": [
				"-y",
				"chrome-devtools-mcp@latest"
			]
		},
		"angular": {
			"command": "npx",
			"args": [
				"angular-mcp",
				"start"
			],
			"type": "stdio"
		},
		"filesystem": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-filesystem",
			]
		}
	},
	"inputs": []
}