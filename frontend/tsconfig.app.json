{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "@angular/localize"
    ],
    "baseUrl": "./src",
    "paths": {
      "@app/*": [
        "app/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@widgets/*": [
        "widgets/*"
      ],
      "@features/*": [
        "features/*"
      ],
      "@entities/*": [
        "entities/*"
      ],
      "@shared/*": [
        "shared/*"
      ],
      "@core/*": [
        "core/*"
      ],
      "@environments/*": [
        "environments/*"
      ],
      "@/*": [
        "*"
      ]
    }
  },
  "files": [
    "./src/main.ts",
  ],
  "include": [
    "src/**/*.d.ts",
    "src/**/*.ts"
  ]
}